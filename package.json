{
  "name": "RoS",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
  "type": "git",
  "url": "https://github.com/calblueprint/ros-student"
  },
  "dependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "css-loader": "0.25.0",
    "node-sass": "3.10.0",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-router": "2.8.1",
    "sass-loader": "4.0.2",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "0.13.1",
    "webpack": "^1.9.11",
    "webpack-dev-middleware": "1.7.0",
    "webpack-dev-server": "^1.9.0"
  },
  "devDependencies": {
    "babel-jest": "15.0.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "jest": "15.1.1",
    "jest-cli": "15.1.1",
    "react-test-renderer": "15.3.1"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "testPathDirs": ["<rootDir>/test/components"],
    "testPathIgnorePatterns": ["/__snapshots__/"],
    "testRegex": "<rootDir>\/test\/components.*|\\.(test|spec)\\.js$",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": ["js", "json","jsx" ],
    "moduleNameMapper": {
       "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/styleMock.js",
      "^.+\\.(css|less|scss)$": "<rootDir>/test/styleMock.js"
    },
    "preprocessorIgnorePatterns": [ "/node_modules/"]
  }
}
